    "esbuild",
    "esno",
    "execa",
    "fetti",
    "fettis",
    "flac",
    "gptr",
    "GRADIO",
    "hideable",
    "hookform",
    "jetbrains",
    "Jinming",
    "keyboardjs",
    "langchain",
    "languagedetector",
    "linebreak",
    "listbox",
    "localstorage",
    "nicepkg",
    "nuxt",
    "OPENAI",
    "OVSE",
    "ovsx",
    "pycache",
    "rehype",
    "rubberband",
    "svgr",
    "tablist",
    "tabpanel",
    "tagify",
    "tanstack",
    "tiktoken",
    "tsup",
    "unconfig",
    "undici",
    "uuidv",
    "vsix",
    "zustand"
